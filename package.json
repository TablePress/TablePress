{
  "name": "TablePress",
  "version": "1.9.2",
  "description": "TablePress enables you to create and manage tables, without having to write HTML code, and it adds valuable functions for your visitors.",
  "repository": {
    "type": "git",
    "url": "https://github.com/TobiasBg/TablePress"
  },
  "author": "Tobias BÃ¤thge",
  "license": "GPL-2.0-only",
  "devDependencies": {
    "autoprefixer": "~9.7.0",
    "grunt": "~1.0.4",
    "grunt-contrib-cssmin": "~3.0.0",
    "grunt-contrib-uglify": "~4.0.1",
    "grunt-contrib-csslint": "~2.0.0",
    "grunt-contrib-jshint": "~2.1.0",
    "grunt-contrib-watch": "~1.1.0",
    "grunt-jsvalidate": "~0.2.2",
    "grunt-postcss": "~0.9.0",
    "grunt-jsonlint": "~2.0.0",
    "matchdep": "~2.0.0"
  },
  "jshintConfig": {
    "boss": true,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "es3": true,
    "expr": true,
